<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19tracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/covid19"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/covid19"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VaccineActivity"></activity>
        <activity android:name=".DetailActivity2" />
        <activity android:name=".AffectedContinents" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".AffectedCountries" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>